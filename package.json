{
  "name": "rubios",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "pod": "cd ios && pod install && cd ../",
    "postinstall": "patch-package && react-native setup-ios-permissions && pod-install",
    "run:android:live": "react-native run-android --mode=LiveDebug --appIdSuffix=live",
    "run:android:live:release": "react-native run-android --mode=LiveRelease --appIdSuffix=live",
    "run:android:prod": "react-native run-android --mode=ProdDebug --appIdSuffix=prod",
    "run:android:staging": "react-native run-android --mode=StagingDebug --appIdSuffix=staging",
    "build:android:live:release": "cd android && ./gradlew assembleLiveRelease",
    "build:android:prod:release": "cd android && ./gradlew assembleProdRelease",
    "build:android:staging:release": "cd android && ./gradlew assembleStagingRelease"
  },
